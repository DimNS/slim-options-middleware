{
    "name": "subjective-php/slim-options-middleware",
    "description": "Middleware to add an OPTIONS route to routes.",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "fig/http-message-util": "^1.1",
        "psr/http-message": "^1.0",
        "slim/slim": "^3.12"
    },
    "require-dev": {
        "chadicus/coding-standard": "^1.7",
        "phpunit/phpunit": "^6.1",
        "satooshi/php-coveralls": "^2.0"
    },
    "config" : {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SubjectivePHP\\Slim\\Middleware\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SubjectivePHPTest\\Slim\\Middleware\\": "tests"
        }
    }
}
